{
  "name": "babel-plugin-antd",
  "version": "0.4.0",
  "description": "Modular antd build plugin for babel.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && ./node_modules/.bin/babel src --out-dir lib --ignore __tests__",
    "test": "babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha --no-timeouts",
    "debug": "mocha --require babel-core/register --require babel-polyfill --no-timeouts",
    "lint": "eslint --ext .js src",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "babel-plugin",
    "antd"
  ],
  "author": {
    "name": "chencheng",
    "email": "sorrycc@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-core": "^6.7.4",
    "babel-istanbul": "^0.7.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "coveralls": "^2.11.6",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "expect": "^1.13.4",
    "mocha": "^2.3.4",
    "pre-commit": "~1.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
